# Optional: Explicitly define Jekyll version if not using github-pages
# gem 'jekyll', '~> 4.0'

group :jekyll_plugins do
  gem 'jekyll-feed'
  gem 'jekyll-sitemap'
  gem 'jekyll-seo-tag'
  gem 'hawkins'
end

# Windows specific gem
gem 'wdm', '~> 0.1.0' if Gem.win_platform?
